    Private Sub Button11_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button11.Click
        MsgBox("The Ducati Panigale V4 is a sport bike with a 1,103 cc desmodromic 90° V4 engine introduced by Ducati in 2018 as the successor to the V-twin engined 1299." & vbNewLine & _
                          "Top Speed; 299 KM/H", MsgBoxStyle.Information, "BIKERS2USED")
    End Sub

    Private Sub Button12_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button12.Click
        MsgBox("The heart of the Panigale V4 R is the new 998 cc Desmosedici Stradale R, an engine capable of reaching a maximum engine speed of 16,500 rpm in sixth gear (16,000 in the other ratios) and delivering a maximum power of 218 hp at 15,500 rpm (Euro-5 compliant*), absolute benchmark values in the panorama of supersports bikes of the same displacement." & vbNewLine & _
                        "Top Speed; 319 KM/H", MsgBoxStyle.Information, "BIKERS2USED")
    End Sub

    Private Sub Button13_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button13.Click
        MsgBox("The Ducati Panigale V2 is a 955 cc V-twin engine sport bike manufactured by Ducati as the successor to the Panigale 959. The motorcycle is named after the manufacturing district of Borgo Panigale. It was announced in 2019 for the 2020 model year." & vbNewLine & _
                          "Top Speed; 270 KM/H", MsgBoxStyle.Information, "BIKERS2USED")
    End Sub

    Private Sub Button14_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button14.Click
        MsgBox("Superleggera V4 is crafted with all the passion, precision, and dedication to perfection Ducati has to offer. Shaping lightness into speed emotions with our most sophisticated engine ever, top of the line components and our most technologically advanced electronics." & vbNewLine & _
                          "Top Speed; 300 KM/H", MsgBoxStyle.Information, "BIKERS2USED")
    End Sub

    Private Sub Button15_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button15.Click
        MsgBox("The Ducati 959 Panigale is a 955 cc sport bike manufactured by Ducati as the successor to the 898 cc 899. The motorcycle is named after the small manufacturing town of Borgo Panigale. It was announced in 2015 for the 2016 model year." & vbNewLine & _
                          "Top Speed; 274 KM/H", MsgBoxStyle.Information, "BIKERS2USED")
    End Sub

    Private Sub Button16_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button16.Click
        MsgBox("The Ducati Monster is a standard, or naked bike, motorcycle designed by Miguel Angel Galluzzi and produced by Ducati in Bologna, Italy, since 1993. In 2005, Monster sales accounted for over half of Ducati's worldwide sales." & vbNewLine & _
                          "Top Speed; 225 KM/H", MsgBoxStyle.Information, "BIKERS2USED")
    End Sub

    Private Sub Button17_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button17.Click
        MsgBox("The Ducati Streetfighter is a streetfighter motorcycle produced by Ducati. The Streetfighter was designed by Damien Basset of the Ducati design team." & vbNewLine & _
                          "Top Speed; 290 KM/H", MsgBoxStyle.Information, "BIKERS2USED")
    End Sub

    Private Sub Button18_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button18.Click
        MsgBox("The Ducati Streetfighter 848 is priced at Rp 290 Million. Streetfighter 848 from Ducati Indonesia comes in Petrol engine with 849cc engine displacement that offers 132@10000 rpmhp of power. The Ducati Streetfighter 848 is available in transmission that has a fuel economy of kmpl. 848 offered in 1 colors: Red. The lowest monthly installment for Ducati Streetfighter 848 is Rp 6,62 Million for November with the downpayment of Rp 72,5 Million. This Sport Bikes competes with. The Other Ducati Streetfighter Variants are: 848." & vbNewLine & _
                          "Top Speed; 256 KM/H", MsgBoxStyle.Information, "BIKERS2USED")
    End Sub

    Private Sub Button19_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button19.Click
        MsgBox("The Ducati 1098 is a sport bike made by Ducati from 2007 to 2009, in three versions, the 1098, 1098S, and 1098R. The 1098 was succeeded by the 1198 in 2009, though the 1098R remained in production that year." & vbNewLine & _
                          "Top Speed; 279 KM/H", MsgBoxStyle.Information, "BIKERS2USED")
    End Sub

    Private Sub Button20_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button20.Click
